#include <LiquidCrystal_I2C.h>
  LiquidCrystal_I2C lcd(0x25, 40, 2);

void setup() {
  Serial.begin(115200);
  

  pinMode(8, OUTPUT);  // disp line select
  digitalWrite(8, HIGH);   // disp line
  lcd.init();              // initialize the LCD
  lcd.setBacklight(HIGH);  // SET LCD LYS ON / OFF
  lcd.clear();             // clear Display


  digitalWrite(8, HIGH);
  lcd.init();              // initialize the LCD

//  lcd.clear();
//  lcd.setCursor(0, 0);
//  lcd.print("1234567890123456789012345678901234567890");
//  lcd.setCursor(0, 1);
//  lcd.print("1234567890123456789012345678901234567890");

  digitalWrite(8, LOW);
  lcd.clear();
  lcd.setCursor(0, 0);
  lcd.print("1234567890123456789012345678901234567890");
  lcd.setCursor(0, 1);
  lcd.print("1234567890123456789012345678901234567890");

}

void loop() {
  pinMode(8, OUTPUT);  // disp line select
  digitalWrite(8, HIGH);   // disp line
  lcd.init();              // initialize the LCD
  lcd.setBacklight(HIGH);  // SET LCD LYS ON / OFF
  lcd.clear();             // clear Display


  digitalWrite(8, HIGH);
  lcd.init();              // initialize the LCD

//  lcd.clear();
//  lcd.setCursor(0, 0);
//  lcd.print("1234567890123456789012345678901234567890");
//  lcd.setCursor(0, 1);
//  lcd.print("1234567890123456789012345678901234567890");

  digitalWrite(8, LOW);
  lcd.clear();
  lcd.setCursor(0, 0);
  lcd.print("1234567890123456789012345678901234567890");
  lcd.setCursor(0, 1);
  lcd.print("1234567890123456789012345678901234567890");
}
